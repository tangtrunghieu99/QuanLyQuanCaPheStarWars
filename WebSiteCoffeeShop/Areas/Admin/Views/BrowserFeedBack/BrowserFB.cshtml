@model PagedList.IPagedList<Model.EF.Feedback>
@using PagedList.Mvc;
@{
    ViewBag.Title = "BrowserFB";
    Layout = "~/Areas/Admin/Views/Shared/_Admin.cshtml";
}

<div class="ibox">
    <div class="ibox-head">
        <div class="ibox-title">Danh sách phản hồi</div>
    </div>
    <div class="ibox-body">
        @*<div class="rel">
                <span class="search-icon"><i class="ti-search"></i></span>
                <input class="form-control" placeholder="Search here...">
            </div>*@
        <div class="row form-group ">
            <div class="col-lg-12">
                <div class="col-md-4">
                    <input type="text" name="Tìm kiếm" class="form-control" placeholder="Tìm kiếm " />
                    <br />
                </div>
                <div class="col-md-4">
                    <button type="submit" class="btn btn-primary">Tìm kiếm</button>
                </div>
            </div>
        </div>
        <table class="table table-striped table-bordered table-hover" id="example-table" cellspacing="0" width="100%">
            <thead>
                <tr>
                    <th>Họ và Tên</th>
                    <th>Nội dung phản hồi</th>
                    <th>Loại tài khoản</th>
                    <th>Đã duyệt</th>
                    <th>Chức năng</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>@item.Name</td>
                        <td>@item.ContentFeedback</td>
                        <td>@item.Type</td>
                        <td>@item.Browser</td>>
                        <td><a href="/duyet-phan-hoi?idFeed=@item.ID">Duyệt</a></td>
                    </tr>
                }
            </tbody>
        </table>
        @Html.PagedListPager(Model, page => Url.Action("BrowserFB", new { page }))
    </div>
</div>


